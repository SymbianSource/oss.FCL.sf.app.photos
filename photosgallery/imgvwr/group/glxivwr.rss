// ---------------------------------------------------------
//
RESOURCE CBA r_glx_default_cba
    {
    buttons=
        {
        CBA_BUTTON
            {
            txt="";
            id=EAknSoftkeyForwardKeyEvent;
            },
        CBA_BUTTON
            {
            txt="";
            id=EAknSoftkeyForwardKeyEvent;
            },
        CBA_BUTTON
            {
            txt="";
            id=EAknSoftkeyForwardKeyEvent;
            }
        };
    }


